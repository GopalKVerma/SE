/* Add here all your CSS customizations */
th {
   background: #eee;
}
.content img {
  margin: auto;
  border:1px solid #d6d6d6;
  max-width: 565px;
}
pre.prettyprint.tryit {
   background: #eee url(/images/link.png) top right no-repeat !important;
}
pre.result, pre.prettyprint {
   width: 100% !important;
   border-radius: 0px;
}
pre {
   padding: 0px;
   margin: 0px;
}
div.result {
   width: 100% !important;
   border:1px solid #d6d6d6;
   border-radius: 0px;
   padding:4px;
}
.box {
   padding:3px;
   margin:3px;
   border:2px solid #aaa;
}
.content h2{
   font-size: 1.6em;
   line-height: 1.5em;
   font-weight: normal;
   position: relative;
   left: 0px;
}
.content h3 {
   font-size: 1.3em;
   line-height: 1.5em;
   text-transform: none;
   color: #000;
   position: relative;
   left: 0px;
}
.content h4 {
   font-size: 1.0em;
   font-weight: bold;
   line-height: 1.5em;
   text-transform: uppercase;
   color: #000;
   position: relative;
   left: 0px;
}
.content ul li a{
   font-weight:bold;
}
.options {
   padding-top:0px !important; 
   margin-top:-10px !important;
}
.hints {
   padding-top:0px !important; 
   margin-top:-15px !important;
}
div.parent-file{
   display:none;
}
ul.alpha, ol.alpha {
   padding-left: 35px;
   list-style: upper-alpha;
}
blockquote {
  padding-left: 0px ;
  margin-left:20px;
  border-left: 4px solid #eee;
  text-align: justify;
}
div.google-map {
   margin: 20px 0 0px 0;
}
